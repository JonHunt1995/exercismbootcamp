// TODO: Set the variables
set x to -1
set y to 0
set hue to 0
set bar_width to 1
set bar_height to 100
set saturation to 50
set luminance to 50

repeat 100 times do
  // Change the variables
  change x to x + 1
change hue to hue + 3
  // TODO: Update the color
  fill_color_hsl(hue, saturation, luminance)
  // TODO: Draw
  rectangle(x, y, bar_width, bar_height)
end